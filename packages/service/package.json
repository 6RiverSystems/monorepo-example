{
  "name": "@sixriver/template-service",
  "version": "0.0.0-development",
  "description": "Sixriver Template Service",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": "10.15.3"
  },
  "scripts": {
    "build": "npm run build:only",
    "rebuild": "npm-run-all clean build",
    "build:apidocs": "lb-apidocs",
    "build:only": "gulp compile --gulpfile '../../repo-tools/build-configs/gulpfile.js' --cwd './'",
    "build:watch": "gulp watch --gulpfile '../../repo-tools/build-configs/gulpfile.js' --cwd './'",
    "clean": "rm -rf dist*",
    "lint": "../../repo-tools/scripts/lint.sh",
    "test": "npm-run-all lint build test-ci",
    "test-pact": "npm-run-all build test-ci-pact",
    "test-ci": "../../repo-tools/scripts/test.sh",
    "test-ci-pact": "NODE_ENV=acceptance ../../repo-tools/scripts/test.sh --opts test/pact.opts",
    "unit": "../../repo-tools/scripts/test.sh --opts test/unit.opts",
    "publish-pacts": "node ../../repo-tools/scripts/publish_pact.js",
    "pretest-ci-pact": "rm -rf pacts",
    "test:no-cover": "NODE_ENV=test mocha ${MOCHA_OPTS}",
    "test:debug": "npm run test:no-cover -- --debug-brk",
    "coverage": "[ ! -d coverage/.nyc_output ] || NODE_ENV=test nyc report --reporter=text-lcov > coverage/lcov.info",
    "prestart": "npm run build:only",
    "start": "USE_GRM=true NODE_ENV=development SITE_NAME=development PUBSUB_EMULATOR_HOST=localhost:8802 PUBSUB_GCLOUD_PROJECT=development node .",
    "release": "../../ci_scripts/ci_tool.sh --docker_publish --docker_file=$HOME/project/common/Dockerfile ",
    "htmlcov:gen": "NODE_ENV=test nyc report --reporter=html",
    "htmlcov:view": "npm run htmlcov:$(bash -c 'echo $OSTYPE')",
    "htmlcov:linux-gnu": "xdg-open ./coverage/.nyc_output/index.html",
    "htmlcov:darwin": "open ./coverage/.nyc_output/index.html",
    "htmlcov": "npm-run-all htmlcov:gen htmlcov:view",
    "db:migrations:create": "f(){ DATABASE_URL=postgres://localhost/template_service_development ./node_modules/.bin/db-migrate create $1 --migrations-dir=migrations/service --sql-file; }; f"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/6RiverSystems/rush-template.git"
  },
  "publishConfig": {
    "registry": "https://sixriver.jfrog.io/sixriver/npm-local/"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist*/src",
    "dist*/index*",
    "src"
  ],
  "dependencies": {
    "@loopback/boot": "1.2.7",
    "@loopback/context": "1.15.0",
    "@loopback/core": "1.7.0",
    "@loopback/openapi-v3": "1.3.11",
    "@loopback/openapi-v3-types": "1.0.18",
    "@loopback/repository": "1.5.5",
    "@loopback/rest": "1.11.2",
    "@loopback/rest-explorer": "1.1.22",
    "@sixriver/config-schema": "1.8.2",
    "@sixriver/loopback4-support": "1.0.18",
    "@sixriver/typescript-support": "1.0.18",
    "@sixriver/service-directory": "1.0.18",
    "@sixriver/template-common": "0.0.0-development",
    "@sixriver/template-oas": "0.0.0-development",
    "ajv": "6.10.0",
    "axios": "0.18.0",
    "bunyan": "1.8.12",
    "db-migrate": "0.11.5",
    "db-migrate-pg": "0.4.0",
    "deep-eql": "3.0.1",
    "instana-nodejs-sensor": "1.49.0",
    "lodash": "4.17.11",
    "loopback-connector-postgresql": "3.6.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "bunyan-prettystream": "0.1.3",
    "@google-cloud/pubsub": "0.28.1",
    "@pact-foundation/pact": "8.2.2",
    "@pact-foundation/pact-node": "8.3.0",
    "@loopback/build": "1.2.0",
    "@loopback/testlab": "1.0.4",
    "@types/bunyan": "1.8.4",
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/chai-string": "1.4.1",
    "@types/chance": "1.0.1",
    "@types/lodash": "4.14.129",
    "@types/mocha": "5.2.6",
    "@types/nock": "9.3.1",
    "@types/node": "10.14.6",
    "@types/sinon": "5.0.2",
    "@types/uuid": "3.4.4",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.4.0",
    "chance": "1.0.13",
    "eslint": "5.4.0",
    "eslint-config-6river": "2.0.0",
    "eslint-config-google": "0.12.0",
    "eslint-plugin-6river": "1.0.6",
    "eslint-plugin-mocha": "5.0.0",
    "mocha": "5.1.1",
    "mocha-junit-reporter": "1.15.0",
    "mocha-lcov-reporter": "1.3.0",
    "nock": "10.0.6",
    "npm-run-all": "4.1.3",
    "nyc": "13.0.1",
    "sinon": "6.3.4",
    "source-map-support": "0.5.9",
    "typescript": "3.4.5"
  }
}
