{
  "name": "@sixriver/template-oas",
  "version": "0.0.0-development",
  "description": "Sixriver Template OpenAPI Schema",
  "keywords": [
  ],
  "main": "dist/index.js",
  "engines": {
    "node": "10.15.1"
  },
  "scripts": {
    "build": "npm-run-all build:utils build:models build:compile",
    "rebuild": "npm-run-all clean build",
    "build:utils": "tsc --project utils",
    "build:compile": "tsc",
    "build:models": "node dist/utils/convertModelsToSchemas.js",
    "clean": "rm -rf dist generated-interfaces",
    "test": "npm-run-all build test-ci",
    "test-pact": "true",
    "test-ci": "npm-run-all lint test:examples",
    "lint": "npm-run-all lint:eslint lint:speccy",
    "lint:eslint": "eslint --ext .ts,.js .",
    "lint:speccy": "speccy lint template-api.yaml",
    "test:examples": "node dist/utils/validateExamplesAgainstSchemas.js",
    "test-ci-pact": "true",
    "unit": "true",
    "publish-pacts": "true",
    "coverage": "true",
    "start": "false",
    "release": "true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/6RiverSystems/rush-monorepo-boilerplate.git"
  },
  "publishConfig": {
    "registry": "https://sixriver.jfrog.io/sixriver/npm-local/"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist/index*",
    "dist/interfaces",
    "dist/schemas",
    "template-api.yaml"
  ],
  "dependencies": {
  },
  "peerDependencies": {
    "ajv": "6.10.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.129",
    "@types/node": "10.14.6",
    "@types/mocha": "5.2.6",
    "@types/swagger-parser": "4.0.3",
    "eslint": "5.4.0",
    "eslint-config-6river": "1.3.0",
    "eslint-config-google": "0.12.0",
    "eslint-plugin-6river": "1.0.6",
    "eslint-plugin-mocha": "5.0.0",
    "eslint-plugin-typescript": "0.9.0",
    "json-schema-to-typescript": "6.1.3",
    "lodash": "4.17.11",
    "npm-run-all": "4.1.3",
    "openapi-schema-to-json-schema": "2.2.0",
    "source-map-support": "0.5.9",
    "speccy": "0.9.0",
    "swagger-parser": "6.0.5",
    "typescript": "3.4.5",
    "typescript-eslint-parser": "22.0.0"
  }
}
