{
	"version": 1,
	"projects": {
		"map-core-app": {
			"root": "apps/map-core-app",
			"sourceRoot": "apps/map-core-app/src",
			"projectType": "application",
			"schematics": {
				"@nrwl/angular:component": {
					"style": "scss"
				}
			},
			"prefix": "sixriver",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/apps/map-core-app",
						"index": "apps/map-core-app/src/index.html",
						"main": "apps/map-core-app/src/main.ts",
						"polyfills": "apps/map-core-app/src/polyfills.ts",
						"tsConfig": "apps/map-core-app/tsconfig.app.json",
						"aot": false,
						"assets": [
							"apps/map-core-app/src/favicon.ico",
							"apps/map-core-app/src/assets",
							{
								"glob": "styles.min.css",
								"input": "node_modules/@shopify/polaris",
								"output": "/"
							},
							{
								"glob": "map.6rms",
								"input": "tools/test-fixtures/maps",
								"output": "/"
							},
							{
								"glob": "empty.6rms",
								"input": "tools/test-fixtures/maps",
								"output": "/"
							},
							{
								"glob": "xpomem.6rms",
								"input": "tools/test-fixtures/maps",
								"output": "/"
							},
							{
								"glob": "selection.6rms",
								"input": "tools/test-fixtures/maps",
								"output": "/"
							}
						],
						"styles": [
							"apps/map-core-app/src/styles.scss",
							"node_modules/@shopify/polaris/styles.min.css"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/map-core-app/src/environments/environment.ts",
									"with": "apps/map-core-app/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": {
								"scripts": true,
								"styles": true,
								"vendor": true
							},
							"extractCss": true,
							"namedChunks": false,
							"aot": true,
							"extractLicenses": true,
							"vendorChunk": true,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "map-core-app:build",
						"sourceMap": {
							"scripts": true,
							"styles": true,
							"vendor": true
						},
						"port": 4202
					},
					"configurations": {
						"production": {
							"browserTarget": "map-core-app:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "map-core-app:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:lint",
					"options": {
						"linter": "eslint",
						"config": ".eslintrc.json",
						"tsConfig": ["apps/map-core-app/tsconfig.app.json"],
						"exclude": ["**/node_modules/**", "!apps/map-core-app/**"]
					}
				}
			}
		},
		"map-core-app-e2e": {
			"root": "apps/map-core-app-e2e",
			"sourceRoot": "apps/map-core-app-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/map-core-app-e2e/cypress.json",
						"tsConfig": "apps/map-core-app-e2e/tsconfig.e2e.json",
						"devServerTarget": "map-core-app:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "map-core-app:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:lint",
					"options": {
						"linter": "eslint",
						"config": ".eslintrc.json",
						"tsConfig": ["apps/map-core-app-e2e/tsconfig.e2e.json"],
						"exclude": ["**/node_modules/**", "!apps/map-core-app-e2e/**"]
					}
				}
			}
		},
