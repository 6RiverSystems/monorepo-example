					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "libs/map-core/test.ts",
						"tsConfig": "libs/map-core/tsconfig.spec.json",
						"karmaConfig": "libs/map-core/karma.conf.js",
						"codeCoverage": true,
						"codeCoverageExclude": ["**/test.ts"],
						"progress": false
					}
				}
			},
			"schematics": {
				"@nrwl/angular:component": {
					"styleext": "scss"
				}
			}
		},
